{
  "name": "@subframe7536/sqlite-wasm",
  "type": "module",
  "version": "0.4.0",
  "description": "typesafe wa-sqlite wrapper, persist data to IndexedDB or OPFS",
  "author": "subframe7536",
  "license": "MIT",
  "repository": "https://github.com/subframe7536/sqlite-wasm",
  "keywords": [
    "typescript",
    "sqlite",
    "wasm",
    "IndexedDB"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./idb": {
      "import": "./dist/idb.js",
      "require": "./dist/idb.cjs"
    },
    "./opfs": {
      "import": "./dist/opfs.js",
      "require": "./dist/opfs.cjs"
    },
    "./constant": {
      "import": "./dist/constant.js",
      "require": "./dist/constant.cjs"
    },
    "./wasm": "./dist/wa-sqlite.wasm",
    "./wasm-async": "./dist/wa-sqlite-async.wasm"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typesVersions": {
    "*": {
      "idb": [
        "./dist/idb.d.ts"
      ],
      "opfs": [
        "./dist/opfs.d.ts"
      ],
      "constant": [
        "./dist/constant.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "tsc --noEmit && eslint ./src --max-warnings 0 --fix",
    "format": "eslint . --fix",
    "dev": "vite serve playground",
    "build": "tsup",
    "release": "pnpm lint && pnpm run build && bumpp --all && npm publish"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@subframe7536/type-utils": "^0.1.6"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.9.4",
    "@types/node": "^20.16.13",
    "bumpp": "^9.7.1",
    "eslint": "^9.13.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "wa-sqlite": "github:rhashimoto/wa-sqlite#v1.0.3",
    "zen-mitt": "^3.0.0"
  }
}
